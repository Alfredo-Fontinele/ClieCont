{
    "name": "sprint5",
    "version": "1.0.0",
    "main": "index.ts",
    "author": "Neto",
    "license": "MIT",
    "scripts": {
        "dev": "ts-node-dev --ignore-watch node_modules src/server.ts",
        "deploy": "yarn && yarn build && yarn typeorm migration:run -d dist/src/data-source.js",
        "drop": "psql -U neto -d tests_products -c \"DROP DATABASE novo\" \"exit\"",
        "new": "psql -U neto -d tests_products -c \"CREATE DATABASE novo\" \"exit\"",
        "typeorm": "typeorm-ts-node-commonjs",
        "build": "tsc",
        "generate": "typeorm-ts-node-commonjs migration:generate -d ./src/data-source.ts src/migrations/createEntities",
        "runner": "typeorm-ts-node-commonjs migration:run -d ./src/data-source.ts",
        "test": "cross-env NODE_ENV=test SECRET_KEY=secret_key jest --verbose --runInBand",
        "start": "node dist/src/server.js"
    },
    "devDependencies": {
        "@types/axios": "^0.14.0",
        "@types/bcryptjs": "^2.4.2",
        "@types/cors": "^2.8.13",
        "@types/express": "^4.17.15",
        "@types/jsonwebtoken": "^8.5.9",
        "@types/supertest": "^2.0.12",
        "ts-node-dev": "^2.0.0",
        "typescript": "^4.7.4"
    },
    "dependencies": {
        "@types/jest": "^28.1.4",
        "bcryptjs": "2.4.3",
        "cors": "^2.8.5",
        "cross-env": "^7.0.3",
        "dotenv": "^16.0.1",
        "express": "^4.18.1",
        "express-async-errors": "^3.1.1",
        "jest": "^28.1.2",
        "jsonwebtoken": "^8.5.1",
        "pg": "^8.7.3",
        "reflect-metadata": "^0.1.13",
        "sqlite3": "^5.0.8",
        "supertest": "^6.2.4",
        "ts-jest": "^28.0.5",
        "ts-node": "^10.9.1",
        "typeorm": "^0.3.7",
        "yup": "0.32.11"
    }
}
